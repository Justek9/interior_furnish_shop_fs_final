generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  id String @id @default(uuid())
  name String
  price Int
  description String
  isNew Boolean
  category String
  imgs ProductImgs[]
   
}

model ProductImgs {
  id String @id @default(uuid())
  photo String
  product Product @relation(fields:[productId], references: [id])
  productId String

}
